{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.16.0",
  "pnpmVersion":  "4.0.2",
  "nodeSupportedVersionRange": ">=8.11.0",
   "projectFolderMinDepth": 2,
   "projectFolderMaxDepth": 2,

  "gitPolicy": {
     "versionBumpCommitMessage": "Applying package updates. [skip-ci]"
  },

  "repository": {
     "url": "https://github.com/Jabher/dothe.git"
  },

  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },


  "projects": [
    {
      "packageName": "get-roots",
      "shouldPublish": true,
      "projectFolder": "libraries/get-roots"
    },
    {
      "packageName": "collections-plus",
      "shouldPublish": true,
      "projectFolder": "libraries/collections-plus"
    },
    {
      "packageName": "dothe",
      "shouldPublish": true,
      "projectFolder": "libraries/dothe"
    },
    {
      "packageName": "dagraph",
      "shouldPublish": true,
      "projectFolder": "libraries/dagraph"
    }
  ]
}
